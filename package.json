  "version": "0.0.3",
  "description": "Additive tweening library for smooth animations",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "webpack",
    "dev": "webpack --watch",
    "lint": "eslint --ext .js .",
